{
  "name": "postcss-custom-props",
  "version": "0.0.0-semantically-released",
  "description": "A PostCSS plugin to enable CSS Custom Properties",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib",
    "commit": "git-cz",
    "lint": "eslint src test",
    "prepublish": "npm run clean && npm run build",
    "test": "mocha --compilers js:babel-core/register test/*.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": {
    "name": "Colin Meinke",
    "email": "hello@colinmeinke.com",
    "url": "colinmeinke.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/colinmeinke/postcss-custom-props"
  },
  "bugs": {
    "url": "https://github.com/colinmeinke/postcss-custom-props/issues"
  },
  "license": "ISC",
  "keywords": [
    "custom",
    "postcss",
    "postcss-plugin",
    "properties",
    "props"
  ],
  "dependencies": {
    "babel-polyfill": "^6.7.4"
  },
  "peerDependencies": {
    "postcss": "^5.0.14"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "commitizen": "^2.7.3",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-config-spaced": "^4.0.1",
    "eslint-plugin-react": "^4.2.3",
    "expect": "^1.16.0",
    "mocha": "^2.4.5",
    "postcss": "^5.0.19",
    "rimraf": "^2.5.2",
    "semantic-release": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}

{
  "name": "postcss-custom-props",
  "version": "0.0.0-semantically-released",
  "description": "A PostCSS plugin to enable CSS Custom Properties",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib",
    "commit": "git-cz",
    "fix": "standard --fix",
    "lint": "standard --verbose | snazzy",
    "prepublish": "npm run clean && npm run build",
    "test": "mocha --compilers js:babel-core/register test/*.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": {
    "name": "Colin Meinke",
    "email": "hello@colinmeinke.com",
    "url": "colinmeinke.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/colinmeinke/postcss-custom-props"
  },
  "bugs": {
    "url": "https://github.com/colinmeinke/postcss-custom-props/issues"
  },
  "license": "ISC",
  "keywords": [
    "custom",
    "postcss",
    "postcss-plugin",
    "properties",
    "props"
  ],
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "cz-conventional-changelog": "^1.2.0"
  },
  "peerDependencies": {
    "postcss": "^5.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.1.5",
    "expect": "^1.20.2",
    "mocha": "^3.1.2",
    "postcss": "^5.2.4",
    "rimraf": "^2.5.4",
    "semantic-release": "^4.3.5",
    "snazzy": "^5.0.0",
    "standard": "^8.4.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
